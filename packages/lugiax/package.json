{
  "name": "@lugia/lugiax",
  "version": "1.0.8",
  "dependencies": {
    "@lugia/lugiax-core": "^1.0.7",
    "immutable": "^3.8.2"
  },
  "peerDependencies": {
    "react": ">16.0.0",
    "react-dom": ">16.0.0"
  },
  "author": "ysstech",
  "scripts": {
    "build": "lugia-complie lint && lugia-complie build",
    "dist": " npm run build && npm publish ./target --registry=http://192.168.102.79:5001/",
    "dev": "./tasks/dev.sh",
    "test": "./tasks/test.sh",
    "cov": "lugia-complie cov",
    "start": "node scripts/start.js",
    "flow": "flow"
  },
  "files": [
    "lib",
    "interface",
    "yarn.lock"
  ],
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": []
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "testMatch": [
      "<rootDir>/target/test/**/*.{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "main": "target/lib/index.js",
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.2.0"
  },
  "devDependencies": {
    "flow-bin": "^0.77.0",
    "react-dev-utils": "4.2.1",
    "react-test-renderer": "16.4.1"
  }
}

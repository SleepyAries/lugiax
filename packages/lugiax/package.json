{
  "name": "@lugia/lugiax",
  "version": "1.0.1-2.1",
  "dependencies": {
    "@lugia/lugiax-core": "^1.0.1-4"
  },
  "author": "ysstech",
  "scripts": {
    "dev": "lugia-complie dev --ig",
    "build": "lugia-complie lint && lugia-complie build",
    "dist": " npm run build && npm publish ./target --registry=http://192.168.102.79:5001/",
    "test": "./tasks/test.sh",
    "cov": "lugia-complie cov",
    "start": "node scripts/start.js",
    "flow": "flow"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "testMatch": [
      "<rootDir>/target/test/**/*.{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.2.0"
  },
  "devDependencies": {
    "flow-bin": "^0.77.0"
  }
}

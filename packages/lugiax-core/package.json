{
  "name": "@lugia/lugiax-core",
  "version": "1.0.1-0",
  "dependencies": {
    "immutable": "^3.8.2",
    "redux": "^4.0.0",
    "redux-immutable": "^4.0.0",
    "redux-saga": "^0.16.0"
  },
  "author": "ysstech",
  "scripts": {
    "dev": "lugia-complie dev --ig",
    "build": "lugia-complie lint && lugia-complie build",
    "dist": " npm run build && npm publish ./target --registry=http://192.168.102.79:5001/",
    "test": "lugia-complie dev --ig & lugia-complie test --watchAll",
    "cov": "lugia-complie cov",
    "start": "node scripts/start.js",
    "flow": "flow"
  },
  "devDependencies": {
    "@lugia/jverify": "1.0.1",
    "@lugia/react-test-utils": "^1.0.0",
    "babel-jest": "20.0.3",
    "debug": "2.6.8",
    "dotenv": "4.0.0",
    "enzyme": "3.2.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "flow-bin": "0.74.0",
    "fs-extra": "3.0.1",
    "jest": "20.0.4",
    "jest-cli": "20.0.4",
    "jsdom": "^11.7.0",
    "lint-staged": "^6.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "testMatch": [
      "<rootDir>/target/test/**/*.{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.2.0"
  }
}

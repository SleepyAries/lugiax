{
  "name": "@lugia/lugiax",
  "version": "1.0.0-beta",
  "dependencies": {
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0"
  },
  "author": "ysstech",
  "scripts": {
    "dev": "lugia-complie dev --ig",
    "build": "lugia-complie lint && lugia-complie build",
    "dist": " npm run build && npm publish ./target --registry=http://192.168.102.79:5001/",
    "test": "lugia-complie dev --ig & lugia-complie test --watchAll",
    "cov": "lugia-complie cov",
    "precommit": "npm run lint-staged",
    "prettier": "prettier --write",
    "lint": "eslint --ext .js src mock tests",
    "lint:fix": "eslint --fix --ext .js src mock tests",
    "lint-staged": "lint-staged",
    "start": "node scripts/start.js",
    "flow": "flow"
  },
  "devDependencies": {
    "@lugia/jverify": "1.0.1",
    "@lugia/lugia-complie": "^1.0.3",
    "autoprefixer": "7.1.6",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chai": "4.1.0",
    "chalk": "1.1.3",
    "cross-env": "5.1.4",
    "css-loader": "0.28.7",
    "debug": "2.6.8",
    "dotenv": "4.0.0",
    "enzyme": "3.2.0",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "flow-bin": "0.74.0",
    "fs-extra": "3.0.1",
    "global": "^4.3.2",
    "gulp": "3.9.1",
    "gulp-babel": "6.1.2",
    "gulp-changed": "3",
    "gulp-flowtype": "1",
    "gulp-newer": "1",
    "gulp-rename": "1.2.2",
    "gulp-uglify": "2",
    "gulp-watch": "4",
    "html-webpack-plugin": "2.29.0",
    "husky": "^0.14.3",
    "identity-obj-proxy": "3.0.0",
    "immutable": "3.8.1",
    "install": "0.10.1",
    "isomorphic-fetch": "2.2.1",
    "jest": "20.0.4",
    "jest-cli": "20.0.4",
    "jest-styled-components": "4.2.2",
    "jsdom": "^11.7.0",
    "lint-staged": "^6.0.0",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "prettier": "^1.13.5",
    "promise": "8.0.1",
    "pump": "1.0.2",
    "raf": "3.4.0",
    "shx": "0.2.2",
    "style-loader": "0.19.0",
    "stylelint": "^9.3.0",
    "stylelint-config-standard": "17.0.0",
    "stylelint-processor-styled-components": "0.2.1",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier",
      "lint:fix",
      "lint",
      "git add"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "testMatch": [
      "<rootDir>/target/test/**/*.{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.2.0"
  }
}
